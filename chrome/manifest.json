{
	"name" : "OpenFaux",
	"description" : "Browser add-on for encrypting and masking internet traffic.",
	"icons" : {
		"16": "media/openfaux-icon-16px.png",
		"48": "media/openfaux-icon-48px.png",
		"128" : "media/openfaux-icon-128px.png"
	},
	"homepage_url" : "https://github.com/openfaux/openfaux-client",
	"background" : {
		"scripts" : ["js/background.js"],
		"persistent" : false
	},
	"browser_action" : {
		"default_title" : "OpenFaux",
		"default_icon" : "media/openfaux-icon-19px.png"
	},
	"version" : "0.1",
	"manifest_version" : 2,
	"commands" : {
		"_execute_browser_action" : {
			"suggested_key" : {
				"default" : "Ctrl+E",
				"mac" : "Command+E"
			}
		}
	},
	"permissions" : ["tabs", "http://*/*"],
	"offline_enabled" : false
}