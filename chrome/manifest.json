{
	"name" : "OpenFaux",
	"description" : "Browser add-on for encrypting and masking internet traffic.",
	"icons" : {
		"128" : "icon128.png"
	},
	"homepage_url" : "https://github.com/openfaux/openfaux-client",
	"background" : {
		"scripts" : ["background.js"],
		"persistent" : false
	},
	"browser_action" : {
		"default_title" : "OpenFaux",
		"default_icon" : "icon2.png"
	},
	"version" : "0.1",
	"manifest_version" : 2,
	"commands" : {
		"_execute_browser_action" : {
			"suggested_key" : {
				"default" : "Ctrl+E",
				"windows" : "Ctrl+E",
				"mac" : "Command+E",
				"chromeos" : "Ctrl+E",
				"linux" : "Ctrl+E"
			}
		}
	},
	"permissions" : ["tabs", "http://*/*"],
	"offline_enabled" : false
}
